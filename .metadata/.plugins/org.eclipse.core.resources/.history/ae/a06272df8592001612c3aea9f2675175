package Restaurant;

public class Packages extends Items {
	double newPrice;
	
	Packages(){
		super();
		newPrice = 0.0;
	}
	
	Packages(String x, double p, double np){
		super(x,p);
		newPrice = np;
	}
	
	Packages(String x, String d, double p,double np ){
		super(x,d,p);
		newPrice = np;
	}
	
	public void changeNewPrice(double np){
		newPrice = np;
	}
}
